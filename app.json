{
  "expo": {
    "name": "Rune-Marker",
    "slug": "Rune-Marker",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#522F18"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "co.polarmushroom.rune_marker"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
